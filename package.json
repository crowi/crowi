  "scripts": {
    "start": "node dist/server/app.js",
    "dev": "concurrently --kill-others 'npm run dev:server' 'npm run dev:client' 'npm run storybook'",
    "dev:server": "DEBUG=crowi:* NODE_ENV=development nodemon lib/app.ts",
    "dev:client": "NODE_ENV=development webpack --progress --profile --watch --config webpack/client/webpack.config.js",
    "format": "eslint --fix '**/*.{ts,tsx,js,jsx}'",
    "lint": "eslint '**/*.{ts,tsx,js,jsx}'",
    "storybook": "start-storybook --ci -s public -p 6001",
    "test": "env -u DEBUG TZ=UTC jest --maxWorkers=5",
    "test:client": "env -u DEBUG TZ=UTC jest client --maxWorkers=5",
    "test:server": "env -u DEBUG jest lib --maxWorkers=5",
    "coverage": "npm test -- --coverage",
    "build": "run-s build:*",
    "build:server": "npm run prod:server",
    "build:client": "npm run prod:client",
    "prod:server": "NODE_OPTIONS=--max_old_space_size=2048 NODE_ENV=production webpack --bail --config webpack/server/webpack.config.js",
    "prod:client": "NODE_ENV=production webpack --bail --config webpack/client/webpack.config.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "webpack": "webpack"
  },
  "env": {
    "NODE_ENV": "production"
  },
  "bugs": {
    "url": "https://github.com/crowi/crowi/issues"
  }
