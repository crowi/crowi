  "scripts": {
    "start": "node app.js",
    "dev": "concurrently --kill-others \"npm run dev:server\" \"npm run dev:client\" \"npm run watch\" ",
    "dev:server": "DEBUG=crowi:* NODE_ENV=development nodemon --watch ./lib/ --watch ./locales/ --ext 'js,json' --exec node app.js",
    "dev:client": "NODE_ENV=development webpack --progress --profile --colors --display-error-details --watch --config tools/webpack.config.js",
    "flow": "flow",
    "format": "eslint --fix \"**/*.js\"",
    "lint": "eslint \"**/*.js\"",
    "test": "mocha -r test/bootstrap.js --globals chai --reporter dot test/**/*.test.js --timeout 10000",
    "build": "npm run prod:client",
    "prod:client": "NODE_ENV=production webpack --bail --colors --display-error-details --config tools/webpack.config.js",
    "webpack": "webpack",
    "watch": "onchange \"**/*.js\" -e \"bin\" -e \"client/thirdparty-js\" -e \"node_modules\" -e \"public\" -f add -f change -- eslint --fix \"{{changed}}\"",
    "postinstall": "npm run build"
  },
  "env": {
    "NODE_ENV": "production"
  },
  "bugs": {
    "url": "https://github.com/crowi/crowi/issues"
  }
